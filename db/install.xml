<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/libguide/db" VERSION="20161229" COMMENT="XMLDB file for Moodle blocks/libguide"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_filescan_files" COMMENT="Filescan results">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="contenthash" TYPE="int" LENGTH="40" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="isChecked" TYPE="boolean" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="status" TYPE="char" LENGTH="1024" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>